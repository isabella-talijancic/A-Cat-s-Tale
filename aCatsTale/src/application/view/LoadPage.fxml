<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: #fee5ed;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoadController">
   <children>
      <ImageView fx:id="bannerImageView" fitHeight="313.0" fitWidth="364.0" layoutX="118.0" layoutY="163.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/loadgif.gif" />
         </image></ImageView>
      <Button fx:id="backButton" layoutX="256.0" layoutY="516.0" mnemonicParsing="false" onAction="#handle" onMouseEntered="#buttonHovered" prefHeight="28.0" prefWidth="88.0" style="-fx-background-color: #deacc4;" text="Game Start!" textAlignment="CENTER">
         <font>
            <Font name="HP Simplified Jpan Regular" size="14.0" />
         </font>
         <effect>
            <Glow>
               <input>
                  <DropShadow />
               </input>
            </Glow>
         </effect>
      </Button>
   </children>
</AnchorPane>
