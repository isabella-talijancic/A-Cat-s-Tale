      <ImageView fx:id="catImageView1" fitHeight="91.0" fitWidth="115.0" layoutX="242.0" layoutY="254.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../images/cat1.png" />
         </image>
      </ImageView>
      <ImageView fx:id="catImageView2" fitHeight="91.0" fitWidth="115.0" layoutX="242.0" layoutY="255.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../images/cat2.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
