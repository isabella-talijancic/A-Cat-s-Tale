<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: #24262b;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LivingRoomController">
