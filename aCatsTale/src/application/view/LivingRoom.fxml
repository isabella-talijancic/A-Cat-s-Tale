            <Font name="Dubai Light" size="40.0" />
         </font>
      </Label>
      <ImageView fx:id="catImageView3" fitHeight="91.0" fitWidth="115.0" layoutX="242.0" layoutY="255.0" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../../images/cat3.png" />
         </image>
      </ImageView>
      <Button fx:id="home" layoutX="518.0" layoutY="22.0" mnemonicParsing="false" onAction="#handle" style="-fx-background-color: #8395b6;" text="Home">
